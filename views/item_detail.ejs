<% if (item.image) { %>
<img
	src="<%=item.image%>"
	alt="<%=item.name%> image"
/>
<% } %>
<h1><%=item.name%></h1>
<h2><%=item.price%> $</h2>
<ul>
	<% for(const [k, v] of item.stats){%>
	<li><%=k%> - <%=v%></li>
	<% } %>
</ul>
<ul>
	<% item.categories.forEach(category => { %>
	<li><a href="<%=category.url%>"><%=category.name%></a></li>
	<% }) %>
</ul>
<ul>
	<% bundle_list.forEach(bundle => { %>
	<li><a href="<%=bundle.url%>"><%=bundle.name%></a></li>
	<% }) %>
</ul>

<a href="<%=item.url%>/update">Update item</a>
<a href="<%=item.url%>/delete">Delete item</a>
